<link href="~/Vendor/font-awesome/css/font-awesome.css" rel="stylesheet" />

<script type="text/javascript">
    $(document).ready(function () {

        $('#treeview').jstree({
            "plugins": ["search", "wholerow"],
            'core': {
                'data': {
                    'url': function (node) {
                        return node.id === '#' ? "/Category/GetRoot" : "/Category/GetChildren/" + node.id;
                    },
                    'data': function (node) {
                        return { 'id': node.id };
                    }
                }
            }
        });
       
